# 环境变量配置示例
# 复制此文件为 .env 并填入真实的配置值

# ============================================================================
# 应用配置
# ============================================================================
APP_ENV=development
LOG_LEVEL=INFO
DATA_DIR=data

# ============================================================================
# 高德地图API
# ============================================================================
GAODE_API_KEY=your_gaode_api_key_here

# ============================================================================
# LLM配置
# ============================================================================
LLM_PROVIDER=openai
LLM_API_KEY=your_llm_api_key_here
LLM_API_BASE=https://api.deepseek.com/v1
LLM_MODEL=deepseek-chat
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=500

